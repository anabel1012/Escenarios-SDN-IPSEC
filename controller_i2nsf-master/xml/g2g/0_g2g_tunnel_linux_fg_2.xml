<ietf-ipsec xmlns="http://example.net/ietf-ipsec" xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0">
<ipsec>
  <spd>
	<spd-entry>
		<rule-number>10</rule-number>
		<priority>0</priority>
		<names>
			<name>in/172.16.1.38/32/172.16.1.86/32</name>
		</names>
		<condition>
			<traffic-selector-list>
				<ts-number>102</ts-number>
				<direction>INBOUND</direction>
				<local-addresses>
					<start>192.168.202.0/24</start>
 					<end>192.168.202.0/24</end>
				</local-addresses>
				<remote-addresses>
					<start>192.168.201.0/24</start>
					<end>192.168.201.0/24</end>
				</remote-addresses>
				<next-layer-protocol>TCP</next-layer-protocol>
				<local-ports>
					<start>0</start>
					<end>0</end>
				</local-ports>
				<remote-ports>
					<start>0</start>
					<end>0</end>
				</remote-ports>
			</traffic-selector-list>
		</condition>
		<processing-info>
			<action>PROTECT</action>
			<ipsec-sa-cfg>
				<security-protocol>esp</security-protocol>
				<mode>TUNNEL</mode>
			<tunnel>
                                <local>172.16.1.38</local>
                                <remote>172.16.1.86</remote>
                        </tunnel>
			</ipsec-sa-cfg>
		</processing-info>
	</spd-entry>
	<spd-entry>
		<rule-number>11</rule-number>
		<priority>0</priority>
		<names>
			<name>out/172.16.1.86/32/172.16.1.38/32</name>
		</names>
		<condition>
			<traffic-selector-list>
				<ts-number>2</ts-number>
				<direction>OUTBOUND</direction>
				<local-addresses>
					<start>192.168.201.0/24</start>
 					<end>192.168.201.0/24</end>
				</local-addresses>
				<remote-addresses>
					<start>192.168.202.0/24</start>
					<end>192.168.202.0/24</end>
				</remote-addresses>
				<next-layer-protocol>TCP</next-layer-protocol>
				<local-ports>
					<start>0</start>
					<end>0</end>
				</local-ports>
				<remote-ports>
					<start>0</start>
					<end>0</end>
				</remote-ports>
			</traffic-selector-list>
		</condition>
		<processing-info>
			<action>PROTECT</action>
			<ipsec-sa-cfg>
				<security-protocol>esp</security-protocol>
				<mode>TUNNEL</mode>
                        <tunnel>
                                <local>172.16.1.86</local>
                                <remote>172.16.1.38</remote>
                        </tunnel>
			</ipsec-sa-cfg>
		</processing-info>
	</spd-entry>
	<spd-entry>
		<rule-number>12</rule-number>
		<priority>0</priority>
		<names>
			<name>fwd/172.16.1.38/32/172.16.1.86/32</name>
		</names>
		<condition>
			<traffic-selector-list>
				<ts-number>102</ts-number>
				<direction>FORWARD</direction>
				<local-addresses>
					<start>192.168.202.0/24</start>
 					<end>192.168.202.0/24</end>
				</local-addresses>
				<remote-addresses>
					<start>192.168.201.0/24</start>
					<end>192.168.201.0/24</end>
				</remote-addresses>
				<next-layer-protocol>TCP</next-layer-protocol>
				<local-ports>
					<start>0</start>
					<end>0</end>
				</local-ports>
				<remote-ports>
					<start>0</start>
					<end>0</end>
				</remote-ports>
			</traffic-selector-list>
		</condition>
		<processing-info>
			<action>PROTECT</action>
			<ipsec-sa-cfg>
				<security-protocol>esp</security-protocol>
				<mode>TUNNEL</mode>
			<tunnel>
                                <local>172.16.1.38</local>
                                <remote>172.16.1.86</remote>
                        </tunnel>
			</ipsec-sa-cfg>
		</processing-info>
	</spd-entry>
  </spd>
<sad>
	<sad-entry>
		<spi>4660</spi> <!-- SPIb1=SPI2 in B-->
		<seq-number>100</seq-number>
                <anti-replay-window>0</anti-replay-window>
		<rule-number>0</rule-number>
		<local-addresses>
			<start>192.168.201.0/24</start>
 			<end>192.168.201.0/24</end>
		</local-addresses>
		<remote-addresses>
			<start>192.168.202.0/24</start>
			<end>192.168.202.0/24</end>
		</remote-addresses>
		<next-layer-protocol>TCP</next-layer-protocol>
		<local-ports>
			<start>0</start>
			<end>0</end>
		</local-ports>
		<remote-ports>
			<start>0</start>
			<end>0</end>
		</remote-ports>
		<security-protocol>esp</security-protocol>
		<esp-sa>
			<encryption>
				<encryption-algorithm>des</encryption-algorithm>
				<key>0000000000000000</key>
			</encryption>
		</esp-sa>
		<mode>TUNNEL</mode>
		<tunnel>
                <local>172.16.1.86</local>
                <remote>172.16.1.38</remote>
	        </tunnel>
	</sad-entry>
	<sad-entry>
		<spi>17185</spi> <!-- SPIa1=SPI1 in A-->
                <anti-replay-window>32</anti-replay-window>
		<rule-number>0</rule-number>
		<local-addresses>
			<start>192.168.202.0/24</start>
 			<end>192.168.202.0/24</end>
		</local-addresses>
		<remote-addresses>
			<start>192.168.201.0/24</start>
			<end>192.168.201.0/24</end>
		</remote-addresses>
		<next-layer-protocol>TCP</next-layer-protocol>
		<local-ports>
			<start>0</start>
			<end>0</end>
		</local-ports>
		<remote-ports>
			<start>0</start>
			<end>0</end>
		</remote-ports>
		<security-protocol>esp</security-protocol>
		<esp-sa>
			<encryption>
				<encryption-algorithm>des</encryption-algorithm>
				<key>0000000000000000</key>
			</encryption>
		</esp-sa>
		<mode>TUNNEL</mode>
		<tunnel>
                <local>172.16.1.38</local>
                <remote>172.16.1.86</remote>
        	</tunnel>
	</sad-entry>
</sad>
</ipsec>
</ietf-ipsec>
